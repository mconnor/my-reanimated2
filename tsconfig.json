{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "baseUrl": [
        "./"
      ]
    },
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
